// Use this file as a starting point for your project's .eslintrc.
// Copy this file, and add rule overrides as needed.
{
  "extends": "airbnb",
  "rules": {
  	 "comma-dangle": [2, "never"], // disallow trailing commas in object literals
			"react/jsx-no-bind": [2, {
			  "ignoreRefs": true,
			  "allowArrowFunctions": true,
			  "allowBind": true
			}],
	"no-param-reassign": 0
	}
}